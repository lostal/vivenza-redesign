{
    "extends": [
        "next/core-web-vitals",
        "next/typescript",
        "prettier"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "warn",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "@typescript-eslint/no-explicit-any": "warn",
        "@next/next/no-page-custom-font": "off"
    }
}